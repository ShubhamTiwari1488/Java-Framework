package com.highradius.pojo;

public class Answer {
	private int aid;
	private String ans;
	public int getAid() {
		return aid;
	}
	public void setAid(int aid) {
		this.aid = aid;
	}
	public String getAns() {
		return ans;
	}
	public void setAns(String ans) {
		this.ans = ans;
	}
	@Override
	public String toString() {
		return "Answer [aid=" + aid + ", ans=" + ans + "]";
	}
	
}
